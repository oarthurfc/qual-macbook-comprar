{
  "kind": "collectionType",
  "collectionName": "mac_books",
  "info": {
    "singularName": "mac-book",
    "pluralName": "mac-books",
    "displayName": "MacBook",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "model": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "screen_size": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "year": {
      "type": "integer",
      "required": true
    },
    "processor": {
      "type": "string",
      "required": true
    },
    "ram_memory": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "storage": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "search_key": {
      "type": "string",
      "required": true,
      "unique": true,
      "private": true
    },
    "relationed_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::relationed-product.relationed-product"
    },
    "macbook_conditions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mac-book-condition.mac-book-condition"
    },
    "comment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::comment.comment"
    }
  }
}
